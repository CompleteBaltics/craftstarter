{
  "name": "completebaltics/craftstarter",
  "description": "A starter boilerplate for Craft 3.x with webpack",
  "version": "1.0.1",
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "nystutios107/craft-minify": "^1.2.9",
    "ether/seo": "^3.1.0",
    "nystudio107/craft-typogrify": "^1.1.16",
    "nystudio107/craft-twigpack": "^1.1.4",
    "ostark/craft-async-queue": "^2.0.0",
    "craftcms/redactor": "^2.3.2",
    "agencyleroy/craft-kint": "^1.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
